{
  "name": "somewhere-2",
  "private": true,
  "workspaces": [
    "forge"
  ],
  "version": "0.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build-contracts": "yarn workspace forge forge-install && yarn workspace forge compile-contracts && yarn typechain --target=ethers-v5 './forge/out/Contract.sol/YourContract.json'",
    "deploy": "cross-env yarn workspace forge deploy-script --private-key $PRIVATE_KEY --fork-url $RPC_URL",
    "fund": "cast send --private-key $PRIVATE_KEY $MY_ADDRESS --value 10ether --rpc-url $RPC_URL"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.6.8",
    "@rainbow-me/rainbowkit": "^0.3.4",
    "@typechain/ethers-v5": "^10.1.0",
    "cross-env": "^7.0.3",
    "eth-hooks": "^4.3.1",
    "ethers": "^5.6.9",
    "next": "^12.1.6",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "typechain": "^8.1.0",
    "wagmi": "^0.4.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "eslint": "^8.15.0",
    "eslint-config-next": "^12.1.6",
    "typescript": "^4.7.2"
  }
}
